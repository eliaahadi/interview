<!DOCTYPE html>
<html>
  <head>
    <title>Airspace</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCi1CV3zcdjqU4kYxm3x9cD2fbreWE6x-E&callback=initMap"
    async defer></script>
  
 
  </head>

  <body>
    <div class="container">
      <% flash.each do |name, msg| %>
        <%= content_tag(:div, msg.html_safe, class: "alert alert-#{name}") %>
      <% end %>

      <%= yield %>
    </div>
  </body>
</html>
